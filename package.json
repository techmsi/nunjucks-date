    "date"
  ],
  "author": "techmsi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/techmsi/nunjucks-date/issues"
  },
  "homepage": "https://github.com/techmsi/nunjucks-date",
  "devDependencies": {
    "commitizen": "*",
    "coveralls": "*",
    "cz-conventional-changelog": "*",
    "jest": "*",
